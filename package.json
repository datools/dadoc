{
  "name": "@datools/dadoc",
  "version": "0.1.0",
  "description": "Framework to build Javascript/Typescript JSON API",
  "keywords": ["framework", "api", "json", "ioc"],
  "repository": {
    "git": "https://github.com/datools/dadoc.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsbuild.json",
    "watch": "tsc -w -p tsbuild.json",
    "lint": "tslint 'src/**/*.ts'",
    "test": "npm run lint && mocha",
    "docs": "rm -rf docs && typedoc --theme minimal --mode file --out docs/api src",
    "prettier": "prettier --ignore-path .gitignore --write './**/*.{js,ts}'",
    "precommit": "npm run prettier && npm run lint",
    "prepublishOnly": "npm test && npm run build"
  },
  "author": "Simon Babay <simon@babay.fr>",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "~2.2.48",
    "@types/superagent": "~3.5.7",
    "@types/supertest": "~2.0.4",
    "mocha": "~5.0.0",
    "source-map-support": "~0.5.3",
    "superagent": "~3.8.2",
    "supertest": "~3.0.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "tslint-config-airbnb": "~5.5.0",
    "typedoc": "~0.10.0",
    "typescript": "~2.7.1"
  },
  "dependencies": {
    "@datools/di": "^0.1.1",
    "@types/body-parser": "~1.16.8",
    "@types/connect": "~3.4.31",
    "@types/debug": "0.0.30",
    "@types/fs-extra": "~5.0.0",
    "@types/js-yaml": "~3.10.1",
    "@types/node": "~9.4.0",
    "body-parser": "~1.18.2",
    "connect": "~3.6.5",
    "debug": "~3.1.0",
    "fast-safe-stringify": "git+https://github.com/davidmarkclements/fast-safe-stringify.git",
    "find-my-way": "~1.10.1",
    "fs-extra": "~5.0.0",
    "js-yaml": "~3.10.0",
    "reflect-metadata": "~0.1.12",
    "url-join": "~4.0.0"
  }
}
