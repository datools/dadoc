{
  "name": "@datools/dadoc",
  "version": "0.1.1",
  "description": "Framework to build Javascript/Typescript JSON API",
  "keywords": [
    "framework",
    "api",
    "json",
    "ioc"
  ],
  "repository": {
    "git": "https://github.com/datools/dadoc.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsbuild.json",
    "watch": "tsc -w -p tsbuild.json",
    "lint": "tslint 'src/**/*.ts'",
    "test": "npm run lint && mocha",
    "docs": "rm -rf docs && typedoc --theme minimal --mode file --out docs/api src",
    "format": "prettier --ignore-path .gitignore --write './**/*.{js,ts}'",
    "precommit": "npm run prettier && npm run lint",
    "prepublishOnly": "npm test && npm run build"
  },
  "author": "Simon Babay <simon@babay.fr>",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "~5.2.0",
    "@types/superagent": "~3.5.8",
    "@types/supertest": "~2.0.4",
    "mocha": "~5.2.0",
    "source-map-support": "~0.5.6",
    "superagent": "~3.8.3",
    "supertest": "~3.1.0",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "tslint-config-airbnb": "~5.9.2",
    "typedoc": "~0.11.1",
    "typescript": "~2.8.3"
  },
  "dependencies": {
    "@datools/di": "~0.1.3",
    "@types/body-parser": "~1.17.0",
    "@types/connect": "~3.4.32",
    "@types/debug": "0.0.30",
    "@types/fs-extra": "~5.0.2",
    "@types/js-yaml": "~3.11.1",
    "@types/node": "~10.1.2",
    "body-parser": "~1.18.3",
    "connect": "~3.6.6",
    "debug": "~3.1.0",
    "fast-safe-stringify": "git+https://github.com/davidmarkclements/fast-safe-stringify.git",
    "find-my-way": "~1.13.0",
    "fs-extra": "~6.0.1",
    "js-yaml": "~3.11.0",
    "reflect-metadata": "~0.1.12",
    "url-join": "~4.0.0"
  }
}
